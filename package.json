{
  "name": "food-safe",
  "version": "1.0.0",
  "description": "Is my food safe ?",
  "main": "server.js",
  "scripts": {
    "encrypt:config": "TYPE=encrypt FILE_TO_BE_ENCRYPTED='./config/default.json' node crypt.js",
    "decrypt:config": "TYPE=decrypt FILE_TO_BE_DECRYPTED='./config/default.json.enc' node crypt.js",
    "start": "node server",
    "server": "nodemon server",
    "client": "npm start --prefix client",
    "dev": "concurrently \"PORT=3000 npm run server\" \"PORT=3001 npm run client\""
  },
  "keywords": [],
  "author": "Dipesh Patil",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.8",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "gravatar": "^1.8.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^6.8.1",
    "node-html-parser": "^6.1.4",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20"
  }
}
